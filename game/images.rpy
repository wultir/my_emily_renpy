init python:
    import math

    def dynamic_animation(st, path, images, padding, delay):
            factor = 1 / delay
            imageNumber = "{:.0f}".format(math.floor(st*factor%images+1)).zfill(padding)
            return Image(path + imageNumber + ".webp")

    def waiting_animation(st, at):
        d = dynamic_animation(st, "fgimage/default/waiting/waiting", 15, 2, 0.1)
        return d, 0.1

    def transsnow_animation(st, at):
        d = dynamic_animation(st, "fgimage/default/Animation/transsnow/transsnow", 241, 3, 0.05)
        return At(d, zoom800x600), 0.05

image waiting = DynamicDisplayable(waiting_animation)
image transsnow = DynamicDisplayable(transsnow_animation)